// glslc --target-spv=spv1.6 shader.rcall -o shader.rcall.spv
#version 460 core
#extension GL_EXT_ray_tracing : require

struct call_payload_struct {
    vec3 col;
};

layout(location = 1) callableDataInEXT call_payload_struct call_payload;

void main() {
    call_payload.col = call_payload.col.grb;
}